pipeline {
    agent none
    stages {
         stage('Build') {
		   agent {
                docker { image 'maven:v1.1' }
           }
            
            steps {
			   
                git credentialsId: 'GitHub', url: 'https://github.com/poojasree/InformationCentre.git'
                sh 'mvn clean install'
            }
         }
                
        }
         }




